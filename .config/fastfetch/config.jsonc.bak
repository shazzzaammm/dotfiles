{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
	"type": "file",
	"source": "~/.config/fastfetch/cat_ascii",
        "padding": {
		"top": 1,
		"right": 5
        },
	"color": {
			"1":"#f5c2e7"
		}
    },

    "display": {
	"color": {
			"keys":"#b4befe"
		}
    },

    "modules": [
        "os",
        "uptime",
        "de",
        "wm",
        "terminal",
        {
            "type": "cpu",
            "showPeCoreCount": true,
            "temp": true
        },
        {
            "type": "gpu",
            "driverSpecific": true,
            "temp": true
        },
    ]
}
